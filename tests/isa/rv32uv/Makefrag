#=======================================================================
# Makefrag for rv32uv tests
#-----------------------------------------------------------------------

rv32uv_sc_tests = \
    vsetivli \
    vsetvl \
    vsetvli_vl_lt_vlmax \
    vsetvli_rs1_eq_zero \
    vmv_v_i

rv32uv_p_tests = $(addprefix rv32uv-p-, $(rv32uv_sc_tests))
rv32uv_v_tests = $(addprefix rv32uv-v-, $(rv32uv_sc_tests))
