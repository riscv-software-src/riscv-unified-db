grammar org.xtext.example.udb.Udb with org.eclipse.xtext.common.Terminals

generate udb "http://www.xtext.org/example/udb/Udb"

Model:
	csrs += Csr*;

/* Taken from Ajit */
Csr:
	'csr' name=ID
		description=STRING
		address=HEX
		reset_value=STRING
		';';
		
/* Terminals */
terminal HEX: 
	('0x' | '0X')
	('0'..'9' | 'A'..'F' | 'a'..'f')
	(('0'..'9' | 'A'..'F' | 'a'..'f' | '_')*('0'..'9' | 'A'..'F' | 'a'..'f'))?
;