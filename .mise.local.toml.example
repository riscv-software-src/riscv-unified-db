# Example local mise configuration
# Copy to .mise.local.toml and customize for your local environment

# Override global settings for local development
[settings]
# Enable experimental features
experimental = true

# Show mise information in shell prompt
#activate_aggressive = true

# Local environment overrides
[env]
# Add local bin directories
#_.path = ["./local/bin", "$PATH"]

# Development-specific settings
#RUST_BACKTRACE = "1"
#LOG_LEVEL = "debug"

# You can override tool versions locally if needed
# [tools]
# node = "20.11.0"  # Use different Node version locally
# ruby = "3.3.0"    # Use different Ruby version locally
